{
  "name": "greencheck",
  "displayName": "Greencheck",
  "version": "0.0.4",
  "description": "GreenCheck tells you at a glance which websites are hosted using green energy straight from your search results.",
  "author": "Kelsie Murphy",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@plasmohq/messaging": "^0.6.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tgwf/co2": "^0.15.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.399.0",
    "next-themes": "^0.3.0",
    "plasmo": "0.88.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@plasmohq/bms": "^2.5.3",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/tgwf__co2": "^0.14.2",
    "@types/webextension-polyfill": "^0.10.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.4",
    "typescript": "5.3.3",
    "webextension-polyfill": "^0.12.0"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "$CRX_KEY",
    "browser_specific_settings": {
      "gecko": {
        "id": "greenchecksupport@proton.me"
      }
    },
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$PLASMO_PUBLIC_CRX_ID"
        ]
      }
    ]
  }
}
